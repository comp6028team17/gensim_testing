ript>\n\t\t<script type=\"text/javascript\" src=\"/archivegrid/assets/js/jquery.cookie.js\"></script>\n\t\t<script type=\"text/javascript\">\n\t\t\tvar qp = \"\";\t\n\t\t\t\n\t\t\tfunction runSearch() {\n\t\t\t\tvar q = $(\"#q\").val();\n\t\t\t\tif (q.length > 0) {\n\t\t\t\t\tvar surl = window.location.href;\n\t\t\t\t\t// remove query terms, if present\n\t\t\t\t\tif (surl.indexOf(\"q=\") > -1) {\n\t\t\t\t\t\tvar surla = surl.substring(0,surl.indexOf(\"q=\"));\n\t\t\t\t\t\tvar surlb = surl.substring(surl.indexOf(\"q=\")+2);\n\t\t\t\t\t\tif (surlb.indexOf(\"&\") > -1) {\n\t\t\t\t\t\t\tsurlb = surlb.substring(surlb.indexOf(\"&\"));\n\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\tsurlb = \"\";\n\t\t\t\t\t\t}\n\t\t\t\t\t\tsurl = surla + surlb;\n\t\t\t\t\t}\n\t\t\t\t\tif (surl.substring(surl.length-1) != \"?\" || surl.indexOf(\"?\") == -1) { surl += \"?\"; }\n\t\t\t\t\tsurl += \"q=\" + escape(q);\n\t\t\t\t\twindow.location.href = surl;\n\t\t\t\t} else {\n\t\t\t\t\t$('#alert').dialog({ title: 'Missing search terms' });\n\t\t\t\t\t$('#alert').html(\"<p>Please enter some words to search.</p>\");\n\t\t\t\t\t$('#alert').dialog('open');\n\t\t\t\t}\n\t\t\t}\n\t\t\t\n\t\t\tfunction dosearch() {\n\t\t\t\t$(\"#searchform\").submit();\n\t\t\t}\n\t\t\t\n\t\t\tfunction erase() {\n\t\t\t\t$(\"#q\").val('');\n\t\t\t}\t\t\n\t\t\t\n\t\t\tfunction nextHighlight(n) {\n\t\t\t\tvar xurl = \"includes/highlight.php?i=\"+n;\n\t\t\t\t$(\"#highlightpanel\").load(xurl);\n\t\t\t}\n\t\t\t\n\t\t\tfunction cltoggle(i) {\n\t\t\t\tif ($(\"#clpart\"+i).is(\":visible\")) {\n\t\t\t\t\t$(\"#clheadlinkid\"+i).css(\"background\",\"url(/archivegrid/assets/img/twistyClosed.gif) no-repeat\");\n\t\t\t\t} else {\n\t\t\t\t\t$(\"#clheadlinkid\"+i).css(\"background\",\"url(/archivegrid/assets/img/twistyOpen.gif) no-repeat\");\n\t\t\t\t}\n\t\t\t\t$(\"#clheading\"+i).css(\"background-color\",\"#EFEFEF\");\t\t\t\t\n\t\t\t\t$(\"#clpart\"+i).toggle('slow');\n\t\t\t}\t\n\t\t\t\n\t\t\tfunction sortFacet(ftype,fsort,foffset) {\n\t\t\t\tvar q = \"\";\n\t\t\t\tvar furl = \"/archivegrid/facet_refresh.php?q=\"+q+\"&facettype=\"+ftype+\"&facetsort=\"+fsort+\"&facetoffset=\"+foffset;\n\t\t\t\tif (fss.length > 0 && fsite.length > 0) {\n\t\t\t\t\tfurl += \"&facetss=\"+fss;\n\t\t\t\t\tfurl += \"&facetsite=\"+fsite;\n\t\t\t\t}\n\t\t\t\t$('#facetwrapper_'+ftype).load(furl);\n\t\t\t}\n\t\t\t\n\t\t\tfunction showQA(id,contributor) {\n\t\t\t\tvar qaload = \"/archivegrid/includes/qastory.php?id=\"+id+\"&contributor=\"+contributor;\n\t\t\t\t$(\"#qa\").load(qaload);\t\t\t\n\t\t\t\t$('#qaModal').modal('show');\t\t\t\n\t\t\t}\n\t\t\t\n\t\t\tfunction searchSelect(i) {\n\t\t\t\t// get just the q query element from the window url\n\t\t\t\tvar qarr = window.location.href.split(\"q=\");\n\t\t\t\tvar q = qarr[1];\n\t\t\t\tif (q.indexOf(\"&\") > -1) {\n\t\t\t\t\tq = q.substring(0,q.indexOf(\"&\"));\n\t\t\t\t}\n\t\t\t\t// unencode parentheses\n\t\t\t\tq = q.replace(/\\%28/g,\"\\(\");\n\t\t\t\tq = q.replace(/\\%29/g,\"\\)\");\n\t\t\t\t// split out any add-on searches\n\t\t\t\tqarr = q.split(\"\\)+AND+\");\n\t\t\t\t// begin rebuilding the query string up to the selected add-on search\n\t\t\t\tvar newq = \"\";\n\t\t\t\tfor(j = 0; j <= i; j++) {\n\t\t\t\t\tif (j > 0) {\n\t\t\t\t\t\tnewq += \")+AND+\";\n\t\t\t\t\t}\t\t\t\n\t\t\t\t\tnewq += qarr[j];\n\t\t\t\t}\n\t\t\t\t// remove preceding ('s \n\t\t\t\tnewq = newq.replace(/^\\(+/, \"\");\n\t\t\t\t// add back in as many as are need (1 fewer than i)\n\t\t\t\tif (i > 0) {\n\t\t\t\t\tfor (k = 0; k < i; k++) {\n\t\t\t\t\t\tnewq = \"(\" + newq;\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t\t// rebuild the window.location.href with the new query string\n\t\t\t\tvar newhrefarr = window.location.href.split(\"q=\");\n\t\t\t\tnewhref = newhrefarr[0] + \"q=\" + newq;\n\t\t\t\tif (newhrefarr[1].indexOf(\"&\") > -1) {\n\t\t\t\t\tnewhref += newhrefarr[1].substring(newhrefarr[1].indexOf(\"&\"));\n\t\t\t\t}\n\t\t\t\t// refresh the window\n\t\t\t\twindow.location.href = newhref;\n\t\t\t}\n\t\t\t\n\t\t\tfunction resetLimit(sel) {\n\t\t\t\tvar limit = sel.options[sel.selectedIndex].value;\n\t\t\t\t// rebuild the window.location.href with the new limit string\n\t\t\t\tvar newhref = window.location.href;\n\t\t\t\tif (newhref.indexOf(\"limit=\") != -1) {\n\t\t\t\t\tvar newhrefarr = newhref.split(\"limit=\");\n\t\t\t\t\tnewhref = newhrefarr[0] + \"limit=\" + limit;\n\t\t\t\t\tif (newhrefarr[1].indexOf(\"&\") != -1) {\n\t\t\t\t\t\tnewhref += newhrefarr[1].substring(newhrefarr[1].indexOf(\"&\"));\n\t\t\t\t\t}\n\t\t\t\t} else {\n\t\t\t\t\tnewhref += \"&limit=\"+limit;\n\t\t\t\t}\n\t\t\t\t// refresh the window\n\t\t\t\twindow.location.href = newhref;\n\t\t\t}\n\t\t\t\n\t\t\tfunction checkHoldings(hid,os) {\n\t\t\t\t$('#checkHoldings').load(\"/archivegrid/includes/check_holdings.php?id=\"+hid+\"&oclcsymbol=\"+os);\n\t\t\t}\n\t\t\t\n\t\t\tfunction are_cookies_enabled() {\n\t\t\t\tvar cookieEnabled = (navigator.cookieEnabled) ? true : false;\n\t\t\t\tif (typeof navigator.cookieEnabled == \"undefined\" && !cookieEnabled) { \n\t\t\t\t\tdocument.cookie=\"testcookie\";\n\t\t\t\t\tcookieEnabled = (document.cookie.indexOf(\"testcookie\") != -1) ? true : false;\n\t\t\t\t}\n\t\t\t\treturn (cookieEnabled);\n\t\t\t}\n\t\t\t\n\t\t\tfunction set_save(cid,rid,rtype) {\n\t\t\t\t\n\t\t\t\tif (are_cookies_enabled()) {\n\t\t\t\t\tvar saved = \"\"\n\t\t\t\t\tvar saved_count = 0;\n\t\t\t\t\tif ($.cookie(\"saved\") != null) {\n\t\t\t\t\t\tsaved = $.cookie(\"saved\");\n\t\t\t\t\t}\n\t\t\t\t\tvar not_saved = true;\n\t\t\t\t\tvar new_saved = cid+\"|\"+rid+\"|\"+rtype;\n\t\t\t\t\tif (saved.length > 0) {\n\t\t\t\t\t\t// see if this has already been saved\n\t\t\t\t\t\tif (saved.indexOf(\":\") != -1) {\n\t\t\t\t\t\t\tvar saved_arr = saved.split(\":\");\n\t\t\t\t\t\t\tvar saved_arr_length = saved_arr.length;\n\t\t\t\t\t\t\tfor (var i = 0; i < saved_arr_length; i++) {\n\t\t\t\t\t\t\t\tif (saved_arr[i] === new_saved) {\n\t\t\t\t\t\t\t\t\tnot_saved = false;\n\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t} else if (saved === new_saved) {\n\t\t\t\t\t\t\tnot_saved = false;\n\t\t\t\t\t\t}\n\t\t\t\t\t}\n\t\t\t\t\tif (saved.length > 0 && not_saved) {\n\t\t\t\t\t\tsaved = saved + \":\";\n\t\t\t\t\t}\n\t\t\t\t\tif (not_saved) {\n\t\t\t\t\t\tsaved = saved + new_saved;\n\t\t\t\t\t\t$.cookie(\"saved\",saved,{ path: '/archivegrid' });\n\t\t\t\t\t\tsaved_arr = saved.split(\":\");\n\t\t\t\t\t\tsaved_count = saved_arr.length;\n\t\t\t\t\t\tvar saved_count_display = \"<span class=\\\"glyphicon glyphicon-shopping-cart\\\"></span> Saved [\"+saved_count+\"]\";\n\t\t\t\t\t\t$(\"#saved_count\").html(saved_count_display);\n\t\t\t\t\t\t$(\"#saved_button_\"+rid).removeClass(\"btn-warning\").addClass(\"btn-default\");\n\t\t\t\t\t\t$(\"#saved_button_\"+rid).html(\"<span class=\\\"glyphicon glyphicon-shopping-cart\\\"></span> Saved\");\n\t\t\t\t\t\tif (saved_count == 1) {\n\t\t\t\t\t\t\t$('#alert_title').html(\"<span class=\\\"glyphicon glyphicon-shopping-cart\\\"></span> Quick Tip for viewing saved records\");\n\t\t\t\t\t\t\t$('#alert_text').html(\"To view the records you've saved, click the <span class=\\\"glyphicon glyphicon-shopping-cart\\\"></span> Saved link in the upper right corner of the page.\");\n\t\t\t\t\t\t\t$('#alert_modal').modal();\n\t\t\t\t\t\t}\n\t\t\t\t\t} else {\n\t\t\t\t\t\t$('#alert_title').html(\"<span class=\\\"glyphicon glyphicon-shopping-cart\\\"></span> This record has already been saved\");\n\t\t\t\t\t\t$('#alert_text').html(\"To view the records you've saved, click the <span class=\\\"glyphicon glyphicon-shopping-cart\\\"></span> Saved link in the upper right corner of the page.<br/><br/>The Saved list will also give you a way to remove selected records from your list.\");\n\t\t\t\t\t\t$('#alert_modal').modal();\n\t\t\t\t\t}\t\n\t\t\t\t} else {\n\t\t\t\t\t$('#alert_title').html(\"<span class=\\\"glyphicon glyphicon-shopping-cart\\\"></span> Cookies are not enabled\");\n\t\t\t\t\t$('#alert_text').html(\"The ArchiveGrid feature for saving records requires the use of temporary, session-based cookies.  Cookies appear to be disabled in your web browser.\");\n\t\t\t\t\t$('#alert_modal').modal();\n\t\t\t\t}\n\t\t\t\treturn false;\n\t\t\t}\n\t\t\t\n\t\t\tfunction delete_save(cid,rid,rtype) {\n\t\t\t\tvar saved = \"\"\n\t\t\t\tvar saved_count = 0;\n\t\t\t\tif ($.cookie(\"saved\") != null) {\n\t\t\t\t\tsaved = $.cookie(\"saved\");\n\t\t\t\t}\n\t\t\t\tvar delete_saved = cid+\"|\"+rid+\"|\"+rtype;\n\t\t\t\tvar new_saved = \"\";\n\t\t\t\tif (saved.length > 0) {\n\t\t\t\t\t// see if this has already been saved\n\t\t\t\t\tif (saved.indexOf(\":\") != -1) {\n\t\t\t\t\t\tvar saved_arr = saved.split(\":\");\n\t\t\t\t\t\tvar saved_arr_length = saved_arr.length;\n\t\t\t\t\t\tfor (var i = 0; i < saved_arr_length; i++) {\n\t\t\t\t\t\t\tif (saved_arr[i] === delete_saved) {\n\t\t\t\t\t\t\t\t// don't add it\n\t\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\t\tif (new_saved.length > 0) {\n\t\t\t\t\t\t\t\t\tnew_saved += \":\";\n\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\tnew_saved += saved_arr[i];\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t}\n\t\t\t\t\t} else if (saved === delete_saved) {\n\t\t\t\t\t\t// leave new_saved empty\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t\tsaved = saved + new_saved;\n\t\t\t\t$.cookie(\"saved\", new_saved, { path: '/archivegrid' });\n\t\t\t\tif (new_saved.length > 0) {\n\t\t\t\t\t// reload the page\n\t\t\t\t\tshow_saved();\n\t\t\t\t} else {\n\t\t\t\t\t// no saved records left!  Go home I guess.\n\t\t\t\t\twindow.location.href = \"/archivegrid\";\n\t\t\t\t}\n\t\t\t\treturn false;\n\t\t\t}\n\t\t\t\n\t\t\tfunction delete_saved_all() {\n\t\t\t\tvar saved = \"\";\n\t\t\t\t$.cookie(\"saved\", saved, { path: '/archivegrid' });\n\t\t\t\twindow.location.href = \"/archivegrid\";\n\t\t\t\treturn false;\n\t\t\t}\n\t\t\t\n\t\t\tfunction show_saved_count() {\n\t\t\t\tvar saved = \"\"\n\t\t\t\tvar saved_count = 0;\n\t\t\t\tif ($.cookie(\"saved\") != null) {\n\t\t\t\t\tsaved = $.cookie(\"saved\");\n\t\t\t\t\tif (saved.length > 0) {\n\t\t\t\t\t\tif (saved.indexOf(\":\") != -1) {\n\t\t\t\t\t\t\