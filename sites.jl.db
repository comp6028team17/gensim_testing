\t\t$.cookie(\"longitude\",longitude);\n\t\t\tinitialLocation = new google.maps.LatLng(latitude,longitude);\n\t\t\tmap.setCenter(initialLocation);\n\t\t\n\t\t} else {\n\t\t\t\n\t\t\tinitialLocation = new google.maps.LatLng($.cookie(\"latitude\"),$.cookie(\"longitude\"));\n\t\t\tmap.setCenter(initialLocation);\n\t\t\t\n\t\t}\n\t  \n\t  function handleNoGeolocation(errorFlag) {\n\t\t\tif (errorFlag == true) {\n\t\t\t\tinitialLocation = defaultLocation;\n\t\t\t} else {\n\t\t\t\tinitialLocation = defaultLocation;\n\t\t\t}\n\t\t\tmap.setCenter(initialLocation);\n\t\t}\n\n\t \t// load the map markers\n\t\tsetupArchiveLocations();\n\n\t\t// reset lat/long cookies for the map center whenever it changes\n\t\tgoogle.maps.event.addListener(map, 'center_changed', function() {\n\t\t\tvar latlng = map.getCenter();\n\t\t\t$.cookie(\"latitude\",latlng.lat());\n\t\t\t$.cookie(\"longitude\",latlng.lng());\n\t\t});\n\t\t\n\t\t// reset zoom cookie for the map zoom level whenever it changes\n\t\tgoogle.maps.event.addListener(map, 'zoom_changed', function() {\n\t\t\tzoom = map.getZoom();\n\t\t\t$.cookie(\"zoom\",zoom);\n\t\t}); \n\t\t\n\t}\n\t\n\tfunction setupArchiveLocations() {\n\n\t  allmarkers.length = 0;\n\t  for (var i in archiveLocations) {\n\t    if (archiveLocations.hasOwnProperty(i)) {\n\t      archive = archiveLocations[i];\n\t      for (var j in archive[\"places\"]) {\n\t        if (archive[\"places\"].hasOwnProperty(j)) {\n\t        \n\t          var place = archive[\"places\"][j];\n\t          var title = place[\"name\"];\n\t          var id = place[\"id\"];\n\t          var web = place[\"web\"];\n\t          \n\t        \t// produce list of archives to select in an accordion\n\t          if (id.length == 0) {\n\t          \talinc++;\n\t          \tif (alinc > 1) {;\n\t          \t\tahtml += \"</div>\\n\";\n\t          \t}\n\t          \t// provide toggles for country/state sections, not for countries with states (US, Canada, Australia)\n\t          \tif (title == \"United States\" || title == \"Canada\" || title == \"Australia\" || title == \"Other Countries\") {\n\t          \t\tahtml += \"<div class=\\\"clhead_region\\\">\"+title+\"</div>\\n\";\n\t          \t} else {\n\t          \t\tahtml += \"<div class=\\\"clhead\\\" id=\\\"clheading\"+alinc+\"\\\"><a href=\\\"javascript:cltoggle('\"+alinc+\"')\\\" class=\\\"clheadlink\\\" id=\\\"clheadlinkid\"+alinc+\"\\\" title=\\\"Section toggle\\\">\"+title+\"</a></div>\\n\";\n\t          \t}\n\t          \tahtml += \"<div class=\\\"clpart\\\" id=\\\"clpart\"+alinc+\"\\\">\\n\";\n\t          } else if ((place[\"latitude\"] != 0 || place[\"latitude\"].length > 0) && (place[\"longitude\"] != 0 || place[\"longitude\"].length > 0)) {\n\t          \tahtml += \"<a href=\\\"javascript:mr('\"+id+\"','\"+place[\"latitude\"]+\"','\"+place[\"longitude\"]+\"')\\\" class=\\\"cllink\\\" title=\\\"View on the map\\\">\"+title+\"</a>\";\n\t          \tahtml += \"\\n\";\n\t          } else {\n\t          \tahtml += \"<a href=\\\"/archivegrid/?q=archive%3A%22\"+escape(title)+\"%22\\\" class=\\\"cllink\\\" title=\\\"View the collections\\\">\"+title+\"</a>\";\n\t          \tahtml += \"\\n\";\n\t          }\n\n\t\t\t\t\t\tvar content = \"<div class=\\\"map_popup\\\">\";\n\t\t\t\t\t\tcontent += \"<div class=\\\"map_text\\\"><strong>\"+title+\"</strong></div>\";\n\t\t\t\t\t\tcontent += \"<div class=\\\"map_text\\\">\";        \n\t\t\t\t\t\tcontent += \"<a href=\\\"?q=contributor:\"+id+\"&sort=titlesort+asc\\\" class=\\\"btn btn-xs btn-success\\\">Search the Collections</a> \";\n\t\t\t\t\t\t//content += \"<a href=\\\"?q=contributor:\"+id+\"&label=\"+escape(title)+\"\\\" class=\\\"btn btn-mini btn-success\\\"><i class=\\\"icon-search icon-white\\\"><i> Search the Collections</a>\";\n\t\t\t\t\t\tcontent += \"</div>\";\n\t\t\t\t\t\tcontent += \"<div class=\\\"map_text\\\">\"; \n\t\t\t\t\t\tcontent += \" \";\n\t\t\t\t\t\tcontent += \"<a href=\\\"ci.php?id=\"+id+\"\\\" class=\\\"btn btn-xs btn-info\\\">Contact Information</a> \";\n\t\t\t\t\t\t//content += \"<a href=\\\"ci.php?id=\"+id+\"\\\" class=\\\"btn btn-info\\\">Contact Information</a>\";\n\t\t\t\t\t\tcontent += \"</div>\";\n\t\t\t\t\t\tcontent += \"</div>\";\n\t\t\t\t\t\tvar posn = null;\n\t\t\t\t\t\t// set position by lat long if available\n\t\t\t\t\t\tif ((place[\"latitude\"] != 0 || place[\"latitude\"].length > 0) && (place[\"longitude\"] != 0 || place[\"longitude\"].length > 0)) {\n\t\t\t\t\t\t\tposn = new google.maps.LatLng(place[\"latitude\"], place[\"longitude\"]);\n\t\t\t\t\t\t\tif (posn) {\n\t\t\t\t\t\t\t\tvar marker = createMarker(posn, title, content);\n\t\t\t\t\t\t\t\tmarker.setMap(map);\n\t\t\t\t\t\t\t\tmarkers.push(marker);\n\t\t\t\t\t\t\t\tallmarkers.push(marker);\n\t\t\t\t\t\t\t\tmarkersById[place[\"id\"]] = marker;\n\t\t\t\t\t\t\t} \n\t\t\t\t\t\t}\n\t        }\n\t      }\n\t    }\n\t  }\n\t  ahtml += \"</div>\\n\";\n\t  $(\"#clscroll\").append(ahtml);\n\t}\n\t\n\tfunction createMarker(posn, title, content) {\n\t  var markerOptions = {\n\t    position: posn,\n\t    title: title,\n\t    content: content\n\t  };\n\t  var marker = new google.maps.Marker(markerOptions);\n\t\tvar infowindow = new google.maps.InfoWindow({content: content, size: new google.maps.Size(50,50)});\n\t\tgoogle.maps.event.addListener(marker, 'click', function() {\n\t\t\tinfowindow.open(map,marker);\n\t\t});  \n\t  return marker;\n\t}\n\t\t\n\tfunction resetLocation() {\t\n\t\tvar zip = $(\"#zip\").val();\n\t\tgeocoder.geocode( { 'address': zip}, function(results, status) {\n      if (status == google.maps.GeocoderStatus.OK) {\n        map.setCenter(results[0].geometry.location);\n        map.setZoom(zoom);\n      } else {\n        $('#alert').html(\"<p>The location \"+zip+\" was not recognized.</p>\");\n\t\t\t\t$('#alert').dialog('open');\n      }\n\t\t});\n\t\treturn false;\n\t} \t\n\t\n\tfunction mr(mid,lat,lng) {\n\t\tvar mrmarker = markersById[mid];\n\t\tmap.setCenter(mrmarker.position);\n\t\tmap.setZoom(zoom);\n\t\tvar infowindow = new google.maps.InfoWindow({content: mrmarker.content, size: new google.maps.Size(50,50)});\n\t\tinfowindow.open(map,mrmarker);\n\t\t$(\"html, body\").animate({\n\t\t\tscrollTop: $(\"#column_middle\").offset().top\n\t\t}, \"slow\");\n\t}\n\t\n\tfunction codeLatLng(callback) {\n\t  if (geocoder) {\n\t    geocoder.geocode({'latLng': defaultLocation}, function(results, status) {\n\t      if (status == google.maps.GeocoderStatus.OK) {\n\t        if (results[1]) {\n\t          callback(results[1].formatted_address);\n\t\t        map.setCenter(results[1].geometry.location);\n\t\t        map.setZoom(zoom);          \n\t        } else {\n\t          alert(\"No results found\");\n\t        }\n\t      } else {\n\t        alert(\"Geocoder failed due to: \" + status);\n\t      }\n\t    });\n\t  }\n\t}  \t\t\n\t\n\t// Shows any overlays currently in the array\n\tfunction showOverlays() {\n\t  if (markersArray) {\n\t    for (i in markersArray) {\n\t      markersArray[i].setMap(map);\n\t    }\n\t  }\n\t}\n</script>\t\t\n\t\t<script type=\"text/javascript\">\n\t\t\t$(document).ready(function(){\n\t\t\t\tif ($(\"#map_canvas\").length > 0) {\n\t\t\t\t\t// there's a map on the page so initialize it\n\t\t\t\t\tinitializeMap();\n\t\t\t\t}\n\n\t\t\t\t// handle links for clicks on background of record in a search result\n\t\t\t\t$( \".record\" ).click(function() {\n\t\t\t\t\tif( ! $( event.target).is('a') ) {\n\t\t\t\t\t\tif (this.id) {\n\t\t\t\t\t\t\tvar url_id = \"url_\"+this.id;\n\t\t\t\t\t\t\twindow.location.href = $(\"#\"+url_id).val();\n\t\t\t\t\t\t}\n  \t\t\t\t}\n\t\t\t\t});\n\t\t\t\t\n\t\t\t\t// set saved counts and buttons\n\t\t\t\tshow_saved_count();\n\t\t\t\tupdate_saved_buttons()\n\t\t\t\t\n\t\t\t});\n\t\t</script>\n\t\t\n\t\t<script type=\"text/javascript\">\n\n  var _gaq = _gaq || [];\n  _gaq.push(['_setAccount', 'UA-18143921-3']);\n  _gaq.push(['_trackPageview']);\n\n  (function() {\n    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = tr